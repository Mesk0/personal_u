    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline\n",
    "\n",
    "a = np.arange(0,15)\n",
    "b = 2*a\n",
    "c = np.stack((a,b))\n",
    "d = np.vstack((a,b))\n",
    "e = np.hstack((a,b))\n",
    "print(c)\n",
    "print(d)\n",
    "print(e)\n",
    "plt.plot(c[0],c[1],color='red')\n",
    "plt.savefig('stack.png')"
